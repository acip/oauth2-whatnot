{
    "name": "acip/oauth2-whatnot",
    "description": "Whatnot Client Provider for The PHP League OAuth2-Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ciprian Amariei",
            "email": "ciprian.amariei@gmail.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "whatnot"
    ],
    "autoload": {
        "psr-4": {
            "Acip\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acip\\OAuth2\\Client\\Tests\\": "tests/src/"
        }
    },
    "require": {
        "php": "^8.0",
        "league/oauth2-client": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "mockery/mockery": "^1.6",
        "squizlabs/php_codesniffer": "^3.10",
        "symfony/var-dumper": "^7.1",
        "spatie/phpunit-watcher": "^1.24"
    },
    "scripts": {
        "test": "phpunit-watcher watch"
    }
}